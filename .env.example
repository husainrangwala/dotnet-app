# Application Settings
# Copy this file to .env and update the values for your environment

# The URL of the endpoint to call periodically
ENDPOINT_URL=https://httpbin.org/delay/1

# How often to call the endpoint (in seconds)
INTERVAL_SECONDS=30

# HTTP request timeout (in seconds)
TIMEOUT_SECONDS=10

# New Relic Configuration
# Set your New Relic license key from: https://one.newrelic.com/admin-portal/api-keys/user-api-key
NEW_RELIC_LICENSE_KEY=your_license_key_here

# Application name as it will appear in New Relic
NEW_RELIC_APP_NAME=EndpointCaller

# Log level for New Relic agent (off, severe, warning, info, fine, finer, finest)
NEW_RELIC_LOG_LEVEL=info
